#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adhamhaithameid/node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules/wxt/bin/node_modules:/Users/adhamhaithameid/node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules/wxt/node_modules:/Users/adhamhaithameid/node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules:/Users/adhamhaithameid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adhamhaithameid/node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules/wxt/bin/node_modules:/Users/adhamhaithameid/node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules/wxt/node_modules:/Users/adhamhaithameid/node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules:/Users/adhamhaithameid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules/wxt/bin/wxt.mjs" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/wxt@0.20.11_@types+node@24.10.1_jiti@2.6.1_lightningcss@1.30.1_rollup@4.52.2/node_modules/wxt/bin/wxt.mjs" "$@"
fi
